{
  "buildCommand": "cd tool-code && npm run build",
  "outputDirectory": "tool-code/dist",
  "framework": null,
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    }
  ]
}
